<wpml-config>
    <custom-fields>

        <custom-field action="ignore">folio_style</custom-field>
        <custom-field action="ignore">folio_cols</custom-field>
        <custom-field action="ignore">krown_page_hero</custom-field>
        <custom-field action="ignore">krown_hero_img</custom-field>
        <custom-field action="ignore">krown_hero_slider</custom-field>
        <custom-field action="ignore">krown_hero_video</custom-field>
        <custom-field action="ignore">krown_hero_show_controls</custom-field>
        <custom-field action="ignore">krown_hero_show_autoplay</custom-field>
        <custom-field action="ignore">krown_hero_show_loop</custom-field>
        <custom-field action="ignore">krown_hero_show_mute</custom-field>
        <custom-field action="ignore">krown_hero_text_tg</custom-field>
        <custom-field action="ignore">krown_hero_text_a</custom-field>
        <custom-field action="translate">lobo_custom_header_html</custom-field>
        <custom-field action="translate">lobo_p_title</custom-field>
        <custom-field action="translate">lobo_p_subtitle</custom-field>
        <custom-field action="translate">krown_hero_text</custom-field>
        <custom-field action="translate">kmb</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">portfolio</custom-type>
    </custom-types>       
    <admin-texts>
        <key name="lobo_compact_menu_text" />
        <key name="lobo_footer_social" />
        <key name="lobo_footer_copy" />
    </admin-texts>    
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>